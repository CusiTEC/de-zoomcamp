@data_loader
def load_data_from_api(*args, **kwargs):
    
    url = 'https://github.com/DataTalksClub/nyc-tlc-data/releases/tag/green/download'

    for month in range(10, 13):
    # Construir la URL del archivo para cada mes
    url = f'https://github.com/DataTalksClub/nyc-tlc-data/releases/download/green/yellow_tripdata_2020-{month:02d}.csv'
    
    # Leer el archivo CSV
    df = pd.read_csv(url)
    
    # Agregar el DataFrame al lista
    df_list.append(df)

    parse_dates = ['tpep_pickup_datetime', 'tpep_dropoff_datetime']

    return pd.read_csv(url, sep=',', compression='gzip', dtype=taxi_dtypes, parse_dates=parse_dates)


